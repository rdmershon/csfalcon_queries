defineTable(query={#event_simpleName="NetworkConnectIP4" RemotePort = 3389}, include=[ComputerName, SourceIP, RemoteIP, RemotePort], name="networktraffic")
| #event_simpleName="ProcessRollup2"
| match(table="networktraffic", field=[ComputerName], column=ComputerName)
| groupBy([ComputerName,SourceIP,UserName,RemoteIP,RemotePort])
| cidr(RemoteIP, subnet=["172.28.0.0/16"])
